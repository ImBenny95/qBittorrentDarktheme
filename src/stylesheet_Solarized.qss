QListView { /* Memo: RSS: torrents, download rules, Execution Log, Options LeftSidebar non item BG */
  background-color: #002B36;
}
QListView:hover {
  border: 1px solid #073642;
  color: #839496;
}
QListView::item { /* Memo: Left Sidebar : each nonactive item (STATUS, TRACKERS) : normal item, Options LeftSidebar item BG */
  padding: 0; /* Add */
  height: 15px; /* Add */
  margin 0; /* Add */
  background-color: #002B36;
  color: #839496;
  height:20px;
}
QTabWidget QListView QLabel { /* Add elm : patch */
  background-color: #eaeaea;
  padding: 0px; /* change for : Excution Log's Line height */
}
QTabWidget QListView QLabel:hover { /* Add elm : patch */
  background-color: #dadada;
}
QListView::item:hover { /* Memo: Left Sidebar : each nonactive item : normal item, Options LeftSidebar item BG */
  background-color: #073642;
  color: #839496;
}

QListView::item:selected,
QListView::item:selected:active {
  color: #2AA198;
  background-color: #073642;
}
QListView::item:selected:hover,
QListView::item:!selected:hover {
  color: #B58900;
  background-color: #073642;
}

QListView::item:!selected {
  color: #839496;
  background-color: #002B36;
}

QTableView::item:selected:active,
QColumnView::item:selected:active {
  color: #2AA198;
  background-color: #002B36;
}

QTableView,
QTableView:hover { /* Add elm : option Advanced  */
  border:0px solid #002B36;
}
