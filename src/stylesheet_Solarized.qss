/* QStyleOptionMenuItem : not reflect*/
QTreeView QMenu::indicator:non-exclusive:checked { /* Menu checkbox: checked */
  image: url(":/qss_icons/rc/checkbox_checked.png");
}

QTableCornerButton::section {
  background-color: #002B36;
  border: 1px transparent #073642;
  border-radius: 0px;
}

QTreeView QMenu::item:first, /* not reflect */
QTreeView QMenu::item:only-one { /* not reflect */
  background-color: #CB4B16;
}

/* QHeaderView ------------------------------------------------------------

https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qheaderview

--------------------------------------------------------------------------- */
QHeaderView {
  background-color: #073642;
  border: 0px transparent #073642;
  border-bottom: 1px solid #586E75;
  padding: 0px;
  margin: 0px;
  border-radius: 0px;
}

QHeaderView:disabled {
  background-color: #073642;
  border: 1px transparent #073642;
  padding: 0px; /* change */
}

QHeaderView::section { /* Memo : transfer list table heading */
  background-color: #073642;
  color: #839496;
  padding: 2px;
  border-radius: 0px;
  text-align: left;
}

QHeaderView::section:checked {
  color: #839496;
  background-color: #D33682;
}

QHeaderView::section:checked:disabled {
  color: #586E75;
  background-color: #D33682;
}

QHeaderView::section::horizontal {
  padding: 0px 4px; /* Add */
  border-left: 1px solid #586E75;
}

QHeaderView::section::horizontal::first, QHeaderView::section::horizontal::only-one {
  border-left: 0px solid #073642; /* change */
}

QHeaderView::section::horizontal:disabled {
  color: #586E75;
}

QHeaderView::section::vertical {
  border-top: 1px solid #586E75; /* change */
}

QHeaderView::section::vertical::first, QHeaderView::section::vertical::only-one {
  border-top: 1px solid #073642;
}

QHeaderView::section::vertical:disabled {
  color: #586E75;
}

QHeaderView::down-arrow {
  /* Those settings (border/width/height/background-color) solve bug */
  /* transparent arrow background and size */
  background-color: #073642;
  color: #586E75;
  height: 12px;
  width: 12px;
  image: url(":/qss_icons/rc/arrow_down.png"); /* used */
}

QHeaderView::up-arrow {
  background-color: #073642;
  height: 12px;
  width: 12px;
  image: url(":/qss_icons/rc/arrow_up.png"); /* used */
}


/* QToolBox --------------------------------------------------------------

https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qtoolbox

--------------------------------------------------------------------------- */
QToolBox { /* Not found below */
  padding: 0px;
  border: 0px;
  border: 1px solid #073642;
}

QToolBox::selected {
  padding: 0px;
  border: 2px solid #CB4B16;
}

QToolBox::tab {
  background-color: #002B36;
  border: 1px solid #073642;
  color: #839496;
  border-top-left-radius: 4px;
  border-top-right-radius: 4px;
}

QToolBox::tab:disabled {
  color: #586E75;
}

QToolBox::tab:selected {
  background-color: #073642;
  border-bottom: 2px solid #CB4B16;
}

QToolBox::tab:selected:disabled {
  background-color: #073642;
  border-bottom: 2px solid #D33682;
}

QToolBox::tab:!selected {
  background-color: #073642;
  border-bottom: 2px solid #073642;
}

QToolBox::tab:!selected:disabled {
  background-color: #002B36;
}

QToolBox::tab:hover {
  border-color: #DC322F;
  border-bottom: 2px solid #DC322F;
}

QToolBox QScrollArea QWidget QWidget {
  padding: 0px;
  border: 0px;
  background-color: #002B36;
}

/* QFrame -----------------------------------------------------------------

https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qframe

--------------------------------------------------------------------------- */
.QFrame { /* Not Main window */
  border-radius: 4px;
  border: 1px solid #073642;
}

.QFrame[frameShape="0"] {
  border-radius: 4px;
  border: 1px transparent #073642;
}

.QFrame[height="3"], .QFrame[width="3"] {
  background-color: #002B36; /* Right Bottom pain separator, Statusbar separator*/
  border: 1px solid #002B36; /* Add */
  width: 0px;  height: 0px;  margin: 0px;  padding: 0px; /* Add */
}

/* QSplitter --------------------------------------------------------------

https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qsplitter

--------------------------------------------------------------------------- */
QSplitter { /* Transfer list Sidebar bg + bottom bar bg,  */
  background-color: #002B36; 
  spacing: 0px;
  padding: 0px;
  margin: 0px;
}

QSplitter::separator { /* Memo: not found */
  background-color: #073642;
  border: 0px solid #002B36;
  spacing: 0px;
  padding: 1px;
  margin: 0px;
}

QSplitter::separator:hover { /* Memo: not found */
  background-color: #586E75;
}

QSplitter::separator:horizontal { /* Memo: not found */
  width: 5px;
  image: url(":/qss_icons/rc/line_vertical.png");
}

QSplitter::separator:vertical { /* Memo: not found */
  height: 5px;
  image: url(":/qss_icons/rc/line_horizontal.png");
}

/* QDateEdit --------------------------------------------------------------

--------------------------------------------------------------------------- */
QDateEdit { /* Memo: not found */
  selection-background-color: #002B36;
  border-style: solid;
  border: 1px solid #073642;
  border-radius: 4px;
  /* This fixes 103, 111 */
  padding-top: 2px;
  /* This fixes 103, 111 */
  padding-bottom: 2px;
  padding-left: 4px;
  padding-right: 4px;
  min-width: 10px;
}

QDateEdit:on {
  selection-background-color: #073642;
}

QDateEdit::drop-down {
  subcontrol-origin: padding;
  subcontrol-position: top right;
  width: 20px;
  border-top-right-radius: 3px;
  border-bottom-right-radius: 3px;
}

QDateEdit::down-arrow {
  image: url(":/qss_icons/rc/arrow_down_disabled.png");
  height: 12px;
  width: 12px;
}

QDateEdit::down-arrow:on, QDateEdit::down-arrow:hover, QDateEdit::down-arrow:focus {
  image: url(":/qss_icons/rc/arrow_down.png");
}

QDateEdit QAbstractItemView {
  background-color: #002B36;
  border-radius: 4px;
  border: 1px solid #073642;
  selection-background-color: #002B36;
}

QAbstractView:hover { /* Memo: not found */
  border: 1px solid #D33682;
  color: #839496;
}

QAbstractView:selected { /* Memo: not found */
  background: #D33682;
  color: #073642;
}

PlotWidget {
  /* Fix cut labels in plots #134 */
  padding: 0px;
}
